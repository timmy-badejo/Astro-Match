{
  "expo": {
    "name": "AstroMatch",
    "slug": "AstroMatch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./vectors/AstroMatchLogo.png",
    "splash": {
      "image": "./vectors/splash-image.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./vectors/ios-splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#FFFFFF"
      }
    },
    "icon": "./vectors/AstroMatchLogo.png",
    "android": {
    "adaptiveIcon": {
    "foregroundImage": "./vectors/AstroMatchLogo.png",
    "backgroundColor": "#FFFFFF"
  }
},

    "web": {
      "favicon": "./vectors/favico.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
